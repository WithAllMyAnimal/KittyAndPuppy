<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_mypage_loading"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/yellow"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_mypage_title_loading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ari"
        android:text="이심\n동심"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@id/toolbar_mypage_loading"
        app:layout_constraintEnd_toEndOf="@id/toolbar_mypage_loading"
        app:layout_constraintStart_toStartOf="@id/toolbar_mypage_loading"
        app:layout_constraintTop_toTopOf="@id/toolbar_mypage_loading" />

    <ImageView
        android:id="@+id/iv_mypage_settings_loading"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:padding="15dp"
        android:src="@drawable/img_mypage_setting"
        app:layout_constraintEnd_toEndOf="@id/toolbar_mypage_loading"
        app:layout_constraintTop_toTopOf="@id/toolbar_mypage_loading"
        app:layout_constraintBottom_toBottomOf="@id/toolbar_mypage_loading"
        />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/con_mypage_loading"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_mypage_loading"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        >

        <ImageView
            android:id="@+id/iv_mypage_loading_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="70dp"
            android:background="@drawable/loading_profile_circle"
            android:layout_marginTop="15dp"
            android:layout_marginStart="15dp"
            app:layout_constraintTop_toTopOf="@id/con_mypage_loading"
            app:layout_constraintStart_toStartOf="@id/con_mypage_loading"
            />

        <ImageView
            android:id="@+id/iv_mypage_loading_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingHorizontal="40dp"
            android:paddingVertical="15dp"
            android:background="#C9C9C9"
            android:layout_marginVertical="15dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toTopOf="@id/iv_mypage_loading_nickname"
            app:layout_constraintStart_toEndOf="@id/iv_mypage_loading_profile"
            app:layout_constraintTop_toTopOf="@id/iv_mypage_loading_profile"
            />

        <ImageView
            android:id="@+id/iv_mypage_loading_nickname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingHorizontal="80dp"
            android:paddingVertical="15dp"
            android:background="#C9C9C9"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toTopOf="@id/iv_mypage_loading_birthday"
            app:layout_constraintTop_toBottomOf="@id/iv_mypage_loading_id"
            app:layout_constraintStart_toEndOf="@id/iv_mypage_loading_profile"
            />

        <ImageView
            android:id="@+id/iv_mypage_loading_birthday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingHorizontal="60dp"
            android:paddingVertical="15dp"
            android:background="#C9C9C9"
            android:layout_marginStart="20dp"
            android:layout_marginVertical="15dp"
            app:layout_constraintTop_toBottomOf="@id/iv_mypage_loading_nickname"
            app:layout_constraintStart_toEndOf="@id/iv_mypage_loading_profile"
            app:layout_constraintBottom_toBottomOf="@id/iv_mypage_loading_profile"
            />

        <ImageView
            android:id="@+id/iv_loading_tab"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingVertical="30dp"
            android:layout_marginTop="30dp"
            android:background="#C9C9C9"
            app:layout_constraintStart_toStartOf="@id/con_mypage_loading"
            app:layout_constraintEnd_toEndOf="@id/con_mypage_loading"
            app:layout_constraintTop_toBottomOf="@id/iv_mypage_loading_profile"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/con_mypage_loading_tag_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingHorizontal="20dp"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@id/iv_loading_tab"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            >

        <ImageView
            android:id="@+id/iv_loading_hospital_tag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/loading_tag"
            android:paddingHorizontal="40dp"
            android:paddingVertical="25dp"
            app:layout_constraintStart_toStartOf="@id/con_mypage_loading_tag_list"
            app:layout_constraintTop_toTopOf="@id/con_mypage_loading_tag_list"
            app:layout_constraintEnd_toStartOf="@id/iv_loading_pet_tag"
            />

        <ImageView
            android:id="@+id/iv_loading_pet_tag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/loading_tag"
            android:layout_marginStart="10dp"
            android:paddingHorizontal="40dp"
            android:paddingVertical="25dp"
            app:layout_constraintTop_toTopOf="@id/con_mypage_loading_tag_list"
            app:layout_constraintEnd_toStartOf="@id/iv_loading_behavior_tag"
            app:layout_constraintStart_toEndOf="@id/iv_loading_hospital_tag"
            />

        <ImageView
            android:id="@+id/iv_loading_behavior_tag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/loading_tag"
            android:layout_marginStart="10dp"
            android:paddingHorizontal="40dp"
            android:paddingVertical="25dp"
            app:layout_constraintTop_toTopOf="@id/con_mypage_loading_tag_list"
            app:layout_constraintEnd_toStartOf="@id/iv_loading_daily_tag"
            app:layout_constraintStart_toEndOf="@id/iv_loading_pet_tag"
            />

        <ImageView
            android:id="@+id/iv_loading_daily_tag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/loading_tag"
            android:layout_marginStart="10dp"
            android:paddingHorizontal="40dp"
            android:paddingVertical="25dp"
            app:layout_constraintEnd_toEndOf="@id/con_mypage_loading_tag_list"
            app:layout_constraintTop_toTopOf="@id/con_mypage_loading_tag_list"
            app:layout_constraintStart_toEndOf="@id/iv_loading_behavior_tag"
            />

        <ImageView
            android:id="@+id/iv_mypage_loading_diary_list_first"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingHorizontal="50dp"
            android:paddingVertical="50dp"
            android:layout_marginTop="20dp"
            android:background="#C9C9C9"
            app:layout_constraintStart_toStartOf="@id/con_mypage_loading_tag_list"
            app:layout_constraintEnd_toStartOf="@id/iv_mypage_loading_diary_list_second"
            app:layout_constraintTop_toBottomOf="@id/iv_loading_hospital_tag"
            />

        <ImageView
            android:id="@+id/iv_mypage_loading_diary_list_second"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingHorizontal="50dp"
            android:paddingVertical="50dp"
            android:layout_marginTop="20dp"
            android:background="#C9C9C9"
            android:layout_marginHorizontal="10dp"
            app:layout_constraintEnd_toStartOf="@id/iv_mypage_loading_diary_list_third"
            app:layout_constraintStart_toEndOf="@id/iv_mypage_loading_diary_list_first"
            app:layout_constraintTop_toBottomOf="@id/iv_loading_hospital_tag"
            />

        <ImageView
            android:id="@+id/iv_mypage_loading_diary_list_third"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingHorizontal="50dp"
            android:paddingVertical="50dp"
            android:layout_marginTop="20dp"
            android:background="#C9C9C9"
            app:layout_constraintEnd_toEndOf="@id/con_mypage_loading_tag_list"
            app:layout_constraintStart_toEndOf="@id/iv_mypage_loading_diary_list_second"
            app:layout_constraintTop_toBottomOf="@id/iv_loading_hospital_tag"
            />

            <ImageView
                android:id="@+id/iv_mypage_loading_diary_list_first_low"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingHorizontal="50dp"
                android:paddingVertical="50dp"
                android:layout_marginTop="20dp"
                android:background="#C9C9C9"
                app:layout_constraintEnd_toStartOf="@id/iv_mypage_loading_diary_list_second_low"
                app:layout_constraintStart_toStartOf="@id/con_mypage_loading_tag_list"
                app:layout_constraintTop_toBottomOf="@id/iv_mypage_loading_diary_list_first"
                />

            <ImageView
                android:id="@+id/iv_mypage_loading_diary_list_second_low"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingHorizontal="50dp"
                android:paddingVertical="50dp"
                android:layout_marginTop="20dp"
                android:background="#C9C9C9"
                android:layout_marginHorizontal="10dp"
                app:layout_constraintStart_toEndOf="@id/iv_mypage_loading_diary_list_first_low"
                app:layout_constraintEnd_toStartOf="@id/iv_mypage_loading_diary_list_third_low"
                app:layout_constraintTop_toBottomOf="@id/iv_mypage_loading_diary_list_second"
                />

            <ImageView
                android:id="@+id/iv_mypage_loading_diary_list_third_low"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingHorizontal="50dp"
                android:paddingVertical="50dp"
                android:layout_marginTop="20dp"
                android:background="#C9C9C9"
                app:layout_constraintStart_toEndOf="@id/iv_mypage_loading_diary_list_second_low"
                app:layout_constraintEnd_toEndOf="@id/con_mypage_loading_tag_list"
                app:layout_constraintTop_toBottomOf="@id/iv_mypage_loading_diary_list_third"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>